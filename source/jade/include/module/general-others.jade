//- UI Elements - General - Others


include ../../common/variables
include ../mixins


//Jumbotron
h3 Jumbotron

div.demo-other
  div.jumbotron.jumbotron-dark(style="background-image:url(#{base_img}/tmp/tmp-2.jpg);")
    h1 Hello, world!
    p This is a simple hero unit, a simple jumbotron-style component for calling extra attention to featured content or information.
    p
      a.btn.btn-success.btn-lg(href="#" role="button") Learn more

//Carousel
h3 Carousel

div.demo-other

  //Basic Carousel
  h4 Basic Carousel

  div#carousel-example-generic.carousel.slide(data-ride="carousel" data-interval="5000" data-pause="hover" data-wrap="true")
    ol.carousel-indicators
      li.active(data-target="#carousel-example-generic" data-slide-to="0")
      li(data-target="#carousel-example-generic" data-slide-to="1")
      li(data-target="#carousel-example-generic" data-slide-to="2")
      li(data-target="#carousel-example-generic" data-slide-to="3")
    div.carousel-inner(role="listbox")
      div.item.active: img(src="#{base_img}carousel/001.jpg" width="100%" alt="No.1")
      div.item: img(src="#{base_img}carousel/002.jpg" width="100%" alt="No.2")
      div.item: img(src="#{base_img}carousel/003.jpg" width="100%" alt="No.3")
      div.item: img(src="#{base_img}carousel/004.jpg" width="100%" alt="No.4")
    a.left.carousel-control(href="#carousel-example-generic" role="button" data-slide="prev")
      span.glyphicon.glyphicon-chevron-left
      span.sr-only Previous
    a.right.carousel-control(href="#carousel-example-generic" role="button" data-slide="next")
      span.glyphicon.glyphicon-chevron-right
      span.sr-only Next

  //Optional captions
  h4 Optional captions

  div#carousel-example-captions.carousel.slide(data-ride="carousel" data-interval="5000" data-pause="hover" data-wrap="true")
    ol.carousel-indicators
      li.active(data-target="#carousel-example-captions" data-slide-to="0")
      li(data-target="#carousel-example-captions" data-slide-to="1")
      li(data-target="#carousel-example-captions" data-slide-to="2")
      li(data-target="#carousel-example-captions" data-slide-to="3")
    div.carousel-inner(role="listbox")
      div.item.active
        img(src="#{base_img}carousel/001.jpg" width="100%" alt="No.1")
        div.carousel-caption
          h3 The sky of Beijing
          p Nulla vitae elit libero, a pharetra augue mollis interdum.
      div.item
        img(src="#{base_img}carousel/002.jpg" width="100%" alt="No.2")
        div.carousel-caption
          h3 The backboard of our community
          p Lorem ipsum dolor sit amet, consectetur adipiscing elit.
      div.item
        img(src="#{base_img}carousel/003.jpg" width="100%" alt="No.3")
        div.carousel-caption
          h3 China Science and Technology Museum
          p China' s only state-level comprehensive Science and Technology museum.
      div.item
        img(src="#{base_img}carousel/004.jpg" width="100%" alt="No.4")
        div.carousel-caption
          h3 This is my dog
          p He's name is "The sixth", he is 1 years old.
    a.left.carousel-control(href="#carousel-example-captions" role="button" data-slide="prev")
      span.glyphicon.glyphicon-chevron-left
      span.sr-only Previous
    a.right.carousel-control(href="#carousel-example-captions" role="button" data-slide="next")
      span.glyphicon.glyphicon-chevron-right
      span.sr-only Next

//Tooltips
h3 Tooltips

div.demo-other
  div.text-left
    +buttons('btn-lg btn-primary mright5', 'Tooltip on top')(data-hover="tooltip" data-placement="top" title="Tooltip on top")
    +buttons('btn-lg btn-success mright5', 'Tooltip on left')(data-hover="tooltip" data-placement="left" title="Tooltip on left")
    +buttons('btn-lg btn-info mright5', 'Tooltip on bottom')(data-hover="tooltip" data-placement="bottom" title="Tooltip on bottom")
    +buttons('btn-lg btn-warning mright5', 'Tooltip on right')(data-hover="tooltip" data-placement="right" title="Tooltip on right")

//Popovers
h3 Popovers

div.demo-other
  div.row
    div.col-md-6
      +buttons('btn-lg btn-danger', 'Click to toggle popover (Right)')(data-toggle="popover" data-placement="right" data-trigger="click" title="Popover title" data-content="And here's some amazing content. It's very engaging. Right?")
    div.col-md-6
      +buttons('btn-lg btn-danger', 'Hover to toggle popover (Left)')(data-toggle="popover" data-placement="left" data-trigger="hover" title="Popover title" data-content="And here's some amazing content. It's very engaging. Right?")
  div.row
    div.col-md-6
      +buttons('btn-lg btn-danger', 'Dismissible popover (Top)', '', '', '', '2')(href="javascript:void(0)" tabindex="0" data-toggle="popover" data-placement="top" data-trigger="focus" title="Dismissible popover" data-content="And here's some amazing content. It's very engaging. Right?")
    div.col-md-6
      +buttons('btn-lg btn-danger', 'Dismissible popover (Bottom)', '', '', '', '2')(href="javascript:void(0)" tabindex="0" data-toggle="popover" data-placement="bottom" data-trigger="focus" title="Dismissible popover" data-content="And here's some amazing content. It's very engaging. Right?")

