      td 2
      td Google
      td 1,138
      td
        div.progress.no-mb: div.progress-bar.progress-bar-success(role="progressbar" data-transitiongoal="21")
    tr.warning
      td 3
      td Yahoo
      td 488
      td
        div.progress.no-mb: div.progress-bar.progress-bar-info(role="progressbar" data-transitiongoal="57")
    tr.danger
      td 4
      td Themeforest
      td 1,290
      td
        div.progress.no-mb: div.progress-bar.progress-bar-warning(role="progressbar" data-transitiongoal="85")
    tr.info
      td 5
      td Evanto
      td 478
      td
        div.progress.no-mb: div.progress-bar.progress-bar-danger(role="progressbar" data-transitiongoal="62")