table.table
  thead
    tr
      th #
      th Entity
      th Value
      th Status
  tbody
    tr.active
      td 1
      td Facebook
      td 573
      td
        div.progress.no-mb: div.progress-bar.progress-bar-primary(role="progressbar" data-transitiongoal="40")
